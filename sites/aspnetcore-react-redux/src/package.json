    "name": "flightsearch",
    "description": "An ASP .NET Core 2 MVC boilerplate with CSV import demonstration.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jonmcquade/aspnetcore-react-redux"
