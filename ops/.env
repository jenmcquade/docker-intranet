## IMPORTANT: DO NOT INCLUDE BLANK LINES IN THIS FILE
## USE COMMENT LINES INSTEAD
#
## Framework
#
IWC_INTRANET_VERSION=1.0.0
DOCKER_ACCT=indiewebconsult
PROC_TYPE=x86
BUILD_TYPE=latest
#
#
## SERVER CONFIGURATIONS FOR GUEST CONTAINERS
## ## These are local to the Docker container
CERT_DIR=/etc/letsencrypt/live
## ## These are local to the host machine
CONFIG_DIR=/intranet/conf
#
#
## NETWORKING
HTTP_ACCESS_PORT=80
HTTP_SECONDARY=8080
SSL_ACCESS_PORT=443
RSYNC_PORT=873
SSH_PORT=2222
SMB_TCP_PORT1=139
SMB_TCP_PORT2=445
SMB_UDP_PORT1=137
SMB_UDP_PORT2=138
DNSCRYPT_TCP=5300
DNSCRYPT_UDP=5300
VNC_TCP_RANGE_START=5900
VNC_TCP_RANGE_END=5911
RTMP_P1=1935
RTMP_P2=1936
PLEX_PORT=32400
## FIREWALL
FW_SERVICE="intranet_firewall"
FW_PORTS="80,443,873,22,139,445,137,138,5300,25,135,587,993,32400,8080,8181,8585,5900,1935"
FW_PROTO="tcp,udp"
FW_STATIC="192.168.1.1"
FW_DISABLE=0
# Your upstream DHCP server, usually your router
DHCP_SERVER_IP=192.168.1.1
#
## NGINX STUFF
#
NGINX_VERSION=1.15.11
NGINX_RTMP_VERSION=1.2.1
FFMPEG_VERSION=4.1.3
#
#
## ENCRYPTION AND SSL
